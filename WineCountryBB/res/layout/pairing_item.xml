<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content"
  android:padding="10dp"
  android:background="@drawable/edges">
<ImageButton 
	android:id="@+id/DelButton_Paring_Item" 
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content"
	android:background="@drawable/delete"
	android:clickable="true"
	android:layout_marginLeft="270dp"
	android:visibility="gone"></ImageButton>
<TextView 
	android:text="@+id/TextView01" 
	android:id="@+id/PairingItem_ID" 
	android:layout_below="@id/DelButton_Paring_Item" 
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content"
	android:visibility="gone"></TextView>
<TextView 
	android:text="Cabernet Sauvignon" 
	android:textColor="@color/title"
	android:textSize="22sp"
	android:id="@+id/PairingItem_Wine" 
	android:paddingLeft="10dp"
	android:paddingBottom="15dp"
	android:layout_below="@id/PairingItem_ID" 
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content"></TextView>
<TableLayout 
	android:id="@+id/TableLayout01" 
	android:layout_marginLeft="15dp"
	android:layout_marginRight="48dp"
	android:paddingBottom="15dp"
	android:layout_below="@id/PairingItem_Wine" 
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content">
		<TableRow 
			android:id="@+id/TableRow01" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:paddingLeft="15dp">
				<ImageButton 
					android:id="@+id/ImageButton01" 
					android:layout_width="30dp" 
					android:layout_height="30dp"
					android:background="@drawable/beef"></ImageButton>
				<TextView 
					android:text="" 
					android:textColor="@color/title"
					android:id="@+id/PairingItem_Meat" 
					android:paddingLeft="15dp"
					android:paddingBottom="9dp"
					android:paddingRight="45dp"
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"></TextView></TableRow>
		<TableRow 
			android:id="@+id/TableRow02" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:paddingLeft="15dp">
				<ImageButton 
					android:id="@+id/ImageButton02" 
					android:layout_width="30dp" 
					android:layout_height="30dp"
					android:background="@drawable/cappuccino"></ImageButton>
				<TextView 
					android:text="" 
					android:textColor="@color/title"
					android:id="@+id/PairingItem_Sauces" 
					android:paddingLeft="15dp"
					android:paddingRight="45dp"
					android:paddingBottom="9dp"
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"></TextView></TableRow>
		<TableRow 
			android:id="@+id/TableRow03" 
			android:layout_below="@id/TableLayout01" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:paddingLeft="15dp">
				<ImageButton 
					android:id="@+id/ImageButton03" 
					android:layout_width="30dp" 
					android:layout_height="30dp"
					android:background="@drawable/cheese"></ImageButton>
				<TextView 
					android:text="" 
					android:textColor="@color/title"
					android:id="@+id/PairingItem_Cheeses" 
					android:paddingLeft="15dp"
					android:paddingRight="45dp"
					android:paddingBottom="9dp"
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"></TextView></TableRow>
		<TableRow 
			android:id="@+id/TableRow04" 
			android:layout_below="@id/TableLayout01" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:paddingLeft="15dp">
				<ImageButton 
					android:id="@+id/ImageButton04" 
					android:layout_width="30dp" 
					android:layout_height="30dp"
					android:background="@drawable/fish"></ImageButton>
				<TextView 
					android:text="" 
					android:textColor="@color/title"
					android:id="@+id/PairingItem_Seafood" 
					android:paddingLeft="15dp"
					android:paddingBottom="9dp"
					android:paddingRight="45dp"
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"></TextView></TableRow>
		<TableRow 
			android:id="@+id/TableRow05" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:paddingLeft="15dp">
				<ImageButton 
					android:id="@+id/ImageButton05" 
					android:layout_width="30dp" 
					android:layout_height="30dp"
					android:background="@drawable/pepper"></ImageButton>
				<TextView 
					android:text="" 
					android:textColor="@color/title"
					android:id="@+id/PairingItem_Veggies" 
					android:paddingLeft="15dp"
					android:paddingBottom="9dp"
					android:paddingRight="45dp"
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"></TextView></TableRow>
</TableLayout>
</RelativeLayout>
