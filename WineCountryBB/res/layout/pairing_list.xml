					android:textSize="17dp"
					android:clickable="true"></TextView>
				<TextView 
					android:text="@string/wine_Brunello" 
					android:id="@+id/wine_Brunello" 
					android:layout_below="@id/wine_Bordeaux" 
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"
					android:padding="10dp"
					android:textSize="17dp"
					android:clickable="true"></TextView>
				<TextView 
					android:text="@string/wine_Cab_Sauvignon" 
					android:id="@+id/wine_Cab_Sauvignon" 
					android:layout_below="@id/wine_Brunello" 
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"
					android:padding="10dp"
					android:textSize="17dp"
					android:clickable="true"></TextView>
				<TextView 
					android:text="@string/wine_Chablis" 
