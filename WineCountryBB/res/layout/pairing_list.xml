<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="230dp"
  android:background="@drawable/edges"
  android:padding="10dp">

<TextView 
	android:text="@string/select_paring" 
	android:id="@+id/TxtRegionTitle" 
	android:layout_width="213dp" 
	android:layout_height="wrap_content"
	android:textSize="20sp"
	android:textColor="@color/title"
	android:paddingBottom="2dp"></TextView>
<ImageButton 
	android:id="@+id/DelButtonPairing" 
	android:layout_toRightOf="@id/TxtRegionTitle" 
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content"
	android:layout_marginLeft="40dp"
	android:background="@drawable/delete"
	android:clickable="true"
	android:visibility="gone"></ImageButton>

<ScrollView 
	android:id="@+id/ScrollView01" 
	android:layout_width="wrap_content" 
	android:layout_height="wrap_content"
	android:layout_below="@id/TxtRegionTitle"
	android:paddingLeft="15dp"
	android:paddingTop="5dp">
		<RelativeLayout 
