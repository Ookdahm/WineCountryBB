					android:layout_below="@id/wine_Shiraz" 
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"
					android:padding="10dp"
					android:textSize="17dp"
					android:clickable="true"></TextView>
				<TextView 
					android:text="@string/wine_Zinfandel" 
					android:id="@+id/wine_Zinfandel" 
					android:layout_below="@id/wine_Valpolicella" 
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"
					android:padding="10dp"
					android:textSize="17dp"
					android:clickable="true"></TextView>
				
					
		</RelativeLayout>
</ScrollView>
</RelativeLayout>
