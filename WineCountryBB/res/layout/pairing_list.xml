			android:layout_width="wrap_content" 
			android:layout_height="wrap_content">
				<TextView 
					android:text="@string/wine_Amarone" 
					android:id="@+id/wine_Amarone" 
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"
					android:padding="10dp"
					android:textSize="17dp"
					android:clickable="true"></TextView>
				<TextView 
					android:text="@string/wine_Barbera" 
					android:id="@+id/wine_Barbera" 
					android:layout_below="@id/wine_Amarone" 
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"
					android:padding="10dp"
					android:textSize="17dp"
					android:clickable="true"></TextView>
				<TextView 
					android:text="@string/wine_Barolo" 
					android:id="@+id/wine_Barolo" 
					android:layout_below="@id/wine_Barbera" 
					android:layout_width="wrap_content" 
					android:layout_height="wrap_content"
					android:padding="10dp"
					android:textSize="17dp"
					android:clickable="true"></TextView>
				<TextView 
					android:text="@string/wine_Beaujolais" 
